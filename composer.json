{
  "name": "wearesho-team/token-generator",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Beton",
      "email": "aleksa.yagich@gmail.com"
    }
  ],
  "scripts": {
    "lint": "phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "phpcbf --standard=PSR2 ./src ./tests",
    "test": "phpunit",
    "cover": "phpunit --coverage-clover=coverage.xml"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Wearesho\\TokenGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\TokenGenerator\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2",
    "horat1us/environment-config": "^1.3",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  }
}
