{
  "name": "wearesho-team/token-generator",
  "description": "Library for generating random tokens",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Beton",
      "email": "aleksa.yagich@gmail.com"
    },
    {
      "name": "Horat1us",
      "email": "reclamme@gmail.com"
    }
  ],
  "scripts": {
    "lint": "phpcs --standard=PSR12 ./src ./tests",
    "phpcbf": "phpcbf --standard=PSR12 ./src ./tests",
    "test": "phpunit",
    "cover": "phpunit --coverage-clover=coverage.xml"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Wearesho\\Token\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\Token\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "horat1us/environment-config": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7"
  }
}
